{
    "name": "golf-os-api",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/golf-os-api/src",
    "projectType": "application",
    "tags": [],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "webpack-cli build",
                "args": ["--node-env=production"]
            },
            "configurations": {
                "development": {
                    "args": ["--node-env=development"]
                }
            }
        },
        "serve": {
            "continuous": true,
            "executor": "@nx/js:node",
            "defaultConfiguration": "development",
            "dependsOn": ["build"],
            "options": {
                "buildTarget": "golf-os-api:build",
                "runBuildTargetDependencies": false
            },
            "configurations": {
                "development": {
                    "buildTarget": "golf-os-api:build:development"
                },
                "production": {
                    "buildTarget": "golf-os-api:build:production"
                }
            }
        }
    }
}
